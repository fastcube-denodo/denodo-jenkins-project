CREATE OR REPLACE SUMMARY VIEW sum1_oracle_store_sales_current (
        ss_sold_date_sk:long (sourcetypeid = '-5', sourcetyperadix = '10', sourcetypedecimals = '0', sourcetypesize = '10'),
        ss_store_sk:long (sourcetypeid = '-5', sourcetyperadix = '10', sourcetypedecimals = '0', sourcetypesize = '10'),
        sum:decimal,
        count_aux:long
    )
    FOLDER = '/07 - summaries'
    ADD SEARCHMETHOD sum1_oracle_store_sales_current(
        I18N us_pst
        CONSTRAINTS (
             ADD ss_sold_date_sk (any) OPT ANY
             ADD ss_store_sk (any) OPT ANY
             ADD sum (any) OPT ANY
             ADD count_aux (any) OPT ANY
        )
        OUTPUTLIST (count_aux, ss_sold_date_sk, ss_store_sk, sum
        )
        WRAPPER (jdbc sum1_oracle_store_sales_current)
    )
AS SELECT ss_sold_date_sk AS ss_sold_date_sk, ss_store_sk AS ss_store_sk, sum(ss_net_profit) AS sum, count(*) AS count_aux FROM oracle_store_sales_current GROUP BY ss_sold_date_sk, ss_store_sk;

CHOWN admin VIEW sum1_oracle_store_sales_current LASTMODIFICATIONUSER admin;

