# #######################################
# POLICY CREATION
# #######################################
CREATE OR REPLACE GLOBAL_SECURITY_POLICIES (credit_card_cvv_policy
        DESCRIPTION = 'Redact all credit card verification values'
        ENABLED = TRUE
        AUDIENCE (
            ALL
        )
        ELEMENTS (
            COLUMNS TAGGED ANY (pii_credit_card_cvv)
        )
        RESTRICTION (
            FILTER = ''
            MASKING ANY (pii_credit_card_cvv) WITH (REDACT) (texts WITH REDACT, datetimes WITH REDACT, numbers WITH REDACT) 
        ),
    credit_card_number_policy
        DESCRIPTION = 'Redact or only show the last 4 digits for restricted users'
        ENABLED = TRUE
        AUDIENCE (
            ALL
        )
        ELEMENTS (
            COLUMNS TAGGED ANY (pii_credit_card_number)
        )
        RESTRICTION (
            FILTER = ''
            MASKING ANY (pii_credit_card_number) WITH (REDACT) (texts WITH LATEST_4, datetimes WITH REDACT, numbers WITH REDACT) 
        ),
    two_factor_authentication_phone_policy
        DESCRIPTION = 'Redact everything by the last 4 digits'
        ENABLED = TRUE
        AUDIENCE (
            ALL
        )
        ELEMENTS (
            VIEW_DATABASES (demo_lite)
            COLUMNS TAGGED ANY (pii_phone)
        )
        RESTRICTION (
            FILTER = ''
            MASKING ANY (pii_phone) WITH (REDACT) (texts WITH LATEST_4, datetimes WITH REDACT, numbers WITH REDACT) 
        ),
    two_factor_email_authentication_policy
        DESCRIPTION = 'Partial redaction of two factor authentication email requests fields'
        ENABLED = TRUE
        AUDIENCE (
            ALL
        )
        ELEMENTS (
            VIEW_DATABASES (demo_lite)
            COLUMNS TAGGED ALL (pii_verification_email)
        )
        RESTRICTION (
            FILTER = ''
            MASKING ANY (pii_verification_email) WITH (CUSTOM = concat(substr(pii_verification_email, 1, 1), '.........', substr(pii_verification_email, (position('@' IN pii_verification_email)-1)))) (texts WITH DEFAULT, datetimes WITH DEFAULT, numbers WITH DEFAULT) 
        ));

CHOWNS (CHOWN admin GLOBAL_SECURITY_POLICY credit_card_cvv_policy LASTMODIFICATIONUSER admin, 
    CHOWN admin GLOBAL_SECURITY_POLICY credit_card_number_policy LASTMODIFICATIONUSER admin, 
    CHOWN admin GLOBAL_SECURITY_POLICY two_factor_authentication_phone_policy LASTMODIFICATIONUSER admin, 
    CHOWN admin GLOBAL_SECURITY_POLICY two_factor_email_authentication_policy LASTMODIFICATIONUSER admin);




